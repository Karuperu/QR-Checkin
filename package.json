{
  "name": "qr-attendance-system",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "concurrently \"npm run dev\" \"npm run dev:qr\"",
    "dev:qr": "vite --config vite.qr.config.ts --port 3000",
    "dev:tunnel": "concurrently \"npm run dev\" \"npm run pagekite:start\"",
    "pagekite:start": "python pagekite.py --config=pagekite.conf",
    "pagekite:stop": "taskkill /f /im python.exe",
    "deploy": "npm run build && npm run pagekite:start"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.54.0",
    "@types/bcryptjs": "^3.0.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "crypto-js": "^4.2.0",
    "express": "^5.1.0",
    "html5-qrcode": "^2.3.8",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.263.1",
    "qrcode": "^1.5.3",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "recharts": "^3.1.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.4.5",
    "@types/qrcode": "^1.5.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "supabase": "^2.30.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^7.0.4"
  }
}
